<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

<form class="form-horizontal" action="/searchMsbMucroom" method="get">    
    <div style="width:50%">
    <h3><%=resource.Title%></h3>
        <input type="hidden" class="form-control" name="rowid" value="0"/>    
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th></th>
                <th></th>
            </tr>
            <tr>
                <td><%=resource.Roomid.label%></td>    
                <td><input type="text" class="form-control" name="roomID"/></td>
                <td>
                    <button type="submit" name="action" value="first" class="btn btn-default">Find</button>
                    <% if(session.user_acl=='ROLE_ADMIN') { %>
                    <button type="submit" class="btn btn-default"><a href="/newMsbMucroom">Add</a></button>
                    <% } %>
                </td>
            </tr>
        </table>    
    </div>
    
    <div style="width:75%">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.MsbMucroom.Roomid.header%></th>
                <th><%=resource.MsbMucroom.Serviceid.header%></th>
                <th><%=resource.MsbMucroom.Creationdate.header%></th>
                <th><%=resource.MsbMucroom.Modificationdate.header%></th>
                <th><%=resource.MsbMucroom.Name.header%></th>
                <th><%=resource.MsbMucroom.Naturalname.header%></th>
                <th><%=resource.MsbMucroom.Description.header%></th>
                <th><%=resource.MsbMucroom.Lockeddate.header%></th>
                <th><%=resource.MsbMucroom.Emptydate.header%></th>
                <th><%=resource.MsbMucroom.Canchangesubject.header%></th>
                <th><%=resource.MsbMucroom.Maxusers.header%></th>
                <th><%=resource.MsbMucroom.Publicroom.header%></th>
                <th><%=resource.MsbMucroom.Moderated.header%></th>
                <th><%=resource.MsbMucroom.Membersonly.header%></th>
                <th><%=resource.MsbMucroom.Caninvite.header%></th>
                <th><%=resource.MsbMucroom.Roompassword.header%></th>
                <th><%=resource.MsbMucroom.Candiscoverjid.header%></th>
                <th><%=resource.MsbMucroom.Logenabled.header%></th>
                <th><%=resource.MsbMucroom.Subject.header%></th>
                <th><%=resource.MsbMucroom.Rolestobroadcast.header%></th>
                <th><%=resource.MsbMucroom.Usereservednick.header%></th>
                <th><%=resource.MsbMucroom.Canchangenick.header%></th>
                <th><%=resource.MsbMucroom.Canregister.header%></th>
                
            	<th>View</th>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <th>Edit</th>
                <th>Delete</th>
                <% } %>
            </tr>
            <% for(var i in records){let row = records[i]; %>			            
            <tr>
                <td><a><%=row.roomID%></a></td>
                <td><a><%=row.serviceID%></a></td>
                <td><a><%=row.creationDate%></a></td>
                <td><a><%=row.modificationDate%></a></td>
                <td><a><%=row.name%></a></td>
                <td><a><%=row.naturalName%></a></td>
                <td><a><%=row.description%></a></td>
                <td><a><%=row.lockedDate%></a></td>
                <td><a><%=row.emptyDate%></a></td>
                <td><a><%=row.canChangeSubject%></a></td>
                <td><a><%=row.maxUsers%></a></td>
                <td><a><%=row.publicRoom%></a></td>
                <td><a><%=row.moderated%></a></td>
                <td><a><%=row.membersOnly%></a></td>
                <td><a><%=row.canInvite%></a></td>
                <td><a><%=row.roomPassword%></a></td>
                <td><a><%=row.canDiscoverJID%></a></td>
                <td><a><%=row.logEnabled%></a></td>
                <td><a><%=row.subject%></a></td>
                <td><a><%=row.rolesToBroadcast%></a></td>
                <td><a><%=row.useReservedNick%></a></td>
                <td><a><%=row.canChangeNick%></a></td>
                <td><a><%=row.canRegister%></a></td>
                
                <td><a href="/readMsbMucroom?roomid=<%=row.roomID%>"><span class="glyphicon glyphicon-open"></span></a></td>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <td><a href="/writeMsbMucroom?roomid=<%=row.roomID%>"><span class="glyphicon glyphicon-edit"></span></a></td>
                <td><a href="/deleteMsbMucroom?roomid=<%=row.roomID%>"><span class="glyphicon glyphicon-trash"></span></a></td>
                <% } %>
            </tr>
            <% } %>            
        </table>
        
        <div class="table-pagination">
           <ul class="pagination">
                 <li><button type="submit" name="action" value="first" class="btn btn-default">First</button></li>
                 <li><button type="submit" name="action" value="previous" class="btn btn-default">Previous</button></li>
                 <li><button type="submit" name="action" value="next" class="btn btn-default">Next</button></li>
                 <li><button type="submit" name="action" value="last" class="btn btn-default">Last</button></li>
           </ul>	        
        </div>
    </div>
</form>                

</section>
</div>
</div>
<% include footer %>
</body>
</html>
