<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/writeMsbMucroom" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.Roomid.label%></td>
                <td><input type="text" class="form-control" name="roomid" value="<%=record.roomID%>"/></td>
            </tr><tr>
                <td><%=resource.Serviceid.label%></td>
                <td><input type="text" class="form-control" name="serviceid" value="<%=record.serviceID%>"/></td>
            </tr><tr>
                <td><%=resource.Creationdate.label%></td>
                <td><input type="text" class="form-control" name="creationdate" value="<%=record.creationDate%>"/></td>
            </tr><tr>
                <td><%=resource.Modificationdate.label%></td>
                <td><input type="text" class="form-control" name="modificationdate" value="<%=record.modificationDate%>"/></td>
            </tr><tr>
                <td><%=resource.Name.label%></td>
                <td><input type="text" class="form-control" name="name" value="<%=record.name%>"/></td>
            </tr><tr>
                <td><%=resource.Naturalname.label%></td>
                <td><input type="text" class="form-control" name="naturalname" value="<%=record.naturalName%>"/></td>
            </tr><tr>
                <td><%=resource.Description.label%></td>
                <td><input type="text" class="form-control" name="description" value="<%=record.description%>"/></td>
            </tr><tr>
                <td><%=resource.Lockeddate.label%></td>
                <td><input type="text" class="form-control" name="lockeddate" value="<%=record.lockedDate%>"/></td>
            </tr><tr>
                <td><%=resource.Emptydate.label%></td>
                <td><input type="text" class="form-control" name="emptydate" value="<%=record.emptyDate%>"/></td>
            </tr><tr>
                <td><%=resource.Canchangesubject.label%></td>
                <td><input type="text" class="form-control" name="canchangesubject" value="<%=record.canChangeSubject%>"/></td>
            </tr><tr>
                <td><%=resource.Maxusers.label%></td>
                <td><input type="text" class="form-control" name="maxusers" value="<%=record.maxUsers%>"/></td>
            </tr><tr>
                <td><%=resource.Publicroom.label%></td>
                <td><input type="text" class="form-control" name="publicroom" value="<%=record.publicRoom%>"/></td>
            </tr><tr>
                <td><%=resource.Moderated.label%></td>
                <td><input type="text" class="form-control" name="moderated" value="<%=record.moderated%>"/></td>
            </tr><tr>
                <td><%=resource.Membersonly.label%></td>
                <td><input type="text" class="form-control" name="membersonly" value="<%=record.membersOnly%>"/></td>
            </tr><tr>
                <td><%=resource.Caninvite.label%></td>
                <td><input type="text" class="form-control" name="caninvite" value="<%=record.canInvite%>"/></td>
            </tr><tr>
                <td><%=resource.Roompassword.label%></td>
                <td><input type="text" class="form-control" name="roompassword" value="<%=record.roomPassword%>"/></td>
            </tr><tr>
                <td><%=resource.Candiscoverjid.label%></td>
                <td><input type="text" class="form-control" name="candiscoverjid" value="<%=record.canDiscoverJID%>"/></td>
            </tr><tr>
                <td><%=resource.Logenabled.label%></td>
                <td><input type="text" class="form-control" name="logenabled" value="<%=record.logEnabled%>"/></td>
            </tr><tr>
                <td><%=resource.Subject.label%></td>
                <td><input type="text" class="form-control" name="subject" value="<%=record.subject%>"/></td>
            </tr><tr>
                <td><%=resource.Rolestobroadcast.label%></td>
                <td><input type="text" class="form-control" name="rolestobroadcast" value="<%=record.rolesToBroadcast%>"/></td>
            </tr><tr>
                <td><%=resource.Usereservednick.label%></td>
                <td><input type="text" class="form-control" name="usereservednick" value="<%=record.useReservedNick%>"/></td>
            </tr><tr>
                <td><%=resource.Canchangenick.label%></td>
                <td><input type="text" class="form-control" name="canchangenick" value="<%=record.canChangeNick%>"/></td>
            </tr><tr>
                <td><%=resource.Canregister.label%></td>
                <td><input type="text" class="form-control" name="canregister" value="<%=record.canRegister%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Update</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default""><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
