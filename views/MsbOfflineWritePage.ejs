<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/writeMsbOffline" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.Subscriber.label%></td>
                <td><input type="text" class="form-control" name="subscriber" value="<%=record.subscriber%>"/></td>
            </tr><tr>
                <td><%=resource.Messageid.label%></td>
                <td><input type="text" class="form-control" name="messageid" value="<%=record.messageID%>"/></td>
            </tr><tr>
                <td><%=resource.Creationdate.label%></td>
                <td><input type="text" class="form-control" name="creationdate" value="<%=record.creationDate%>"/></td>
            </tr><tr>
                <td><%=resource.Messagesize.label%></td>
                <td><input type="text" class="form-control" name="messagesize" value="<%=record.messageSize%>"/></td>
            </tr><tr>
                <td><%=resource.Stanza.label%></td>
                <td><input type="text" class="form-control" name="stanza" value="<%=record.stanza%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Update</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default""><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
